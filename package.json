{
  "name": "react-native-actionsheet-ex",
  "version": "1.0.3",
  "description": "Cross platform ActionSheet. This component implements a custom ActionSheet  and provides the same way to drawing it on the defferent platforms(iOS and Android). Actually, In order to keep the best effect, it still uses the ActionSheetIOS on iOS. Based on unmaintained react-native-actionsheet",
  "main": "lib/index.js",
  "scripts": {
    "lint": "standard lib/**/*.js",
    "test": "jest --no-cache"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/Examples"
    ]
  },
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arjan-zuidema/react-native-actionsheet-ex.git"
  },
  "keywords": [
    "actionsheet",
    "action sheet",
    "react-native",
    "react-native-action-sheet",
    "react-native-actionsheet",
    "react-native-action-sheet-ex",
    "react-native-actionsheet-ex",
    "custom-action-sheet"
  ],
  "author": "Arjan Zuidema",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arjan-zuidema/react-native-actionsheet-ex/issues"
  },
  "homepage": "https://github.com/arjan-zuidema/react-native-actionsheet-ex#readme",
  "peerDependencies": {
    "prop-types": ">=15.4.0",
    "react": ">=15.4.0",
    "react-native": "*"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "^21.2.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react-native": "^4.0.0",
    "jest": "^21.2.1",
    "react-test-renderer": "15.4.0",
    "standard": "*"
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
